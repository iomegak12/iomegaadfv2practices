{
	"name": "pipeline3",
	"properties": {
		"activities": [
			{
				"name": "ML Batch Execution1",
				"type": "AzureMLBatchExecution",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"webServiceInputs": {
						"input1": {
							"filePath": "data/sample-hw.csv",
							"linkedServiceName": {
								"referenceName": "AzureBlobStorage2",
								"type": "LinkedServiceReference"
							}
						}
					},
					"webServiceOutputs": {
						"output1": {
							"filePath": "/data/outputs.csv",
							"linkedServiceName": {
								"referenceName": "AzureBlobStorage5",
								"type": "LinkedServiceReference"
							}
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureML1",
					"type": "LinkedServiceReference"
				}
			}
		]
	}
}